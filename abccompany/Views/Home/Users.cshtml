@model IEnumerable<abccompany.Models.user>
<section>
    <div class="row">
        <div class="col-12 my-3">
            <h2>Users</h2>
            <h3>@Html.Raw(TempData["recodeDelete"])</h3>
        </div>
        <div class="col-12">
            <button type="button" class="btn btn-dark">@Html.ActionLink("Create New", "Create")</button>
        </div>
        <div class="col-12">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Name</th>
                        <th scope="col">Contact</th>
                        <th scope="col">Gender</th>
                        <th scope="col">Email</th>
                    </tr>
                </thead>
                @foreach (var item in Model)
                {
                    <tbody class="">
                        <tr>
                            <th scope="row">@Html.DisplayFor(modelItem => item.userID)</th>
                            <td>@Html.DisplayFor(modelItem => item.userName)</td>
                            <td>@Html.DisplayFor(modelItem => item.userContact)</td>
                            <td>@Html.DisplayFor(modelItem => item.userGender)</td>
                            <td>@Html.DisplayFor(modelItem => item.userEmail)</td>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { id = item.userID }, new { @class = "btn btn-primary" })
                                @Html.ActionLink("Details", "Detail", new { id = item.userID }, new { @class = "btn btn-primary" })
                                @Html.ActionLink("Delete", "Delete", new { id = item.userID }, new { @class = "btn btn-danger delete", onclick = "return confirm('Are sure wants to delete?');" })
                            </td>
                        </tr>
                    </tbody>
                }
            </table>
        </div>
    </div>
</section>
